

<% if logged_in? %>
<h1>Teespring Tickets
<%= link_to "Create a New Ticket", new_article_path, class: "new_article" %></h1>
<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article),
          class: 'article_title', id: "article_#{article.id}" %>
    </li>
  <% end %>
<%end%>

</ul>
